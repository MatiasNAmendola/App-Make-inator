#tag WindowCode
	#tag Event
		Sub Open()
		  popVariablesMath3.AddRow "Numbers"
		  popVariablesMath3.AddRow "Text"
		  popVariablesMath3.SelectVariableByName(ilObjectIfThen(frmProject.chartProject.selectedFrame.tag).MathVariable3)
		  
		  if popVariablesMath3.Text = "" then
		    popVariablesMath3.SelectVariableByName("Numbers")
		  end if
		  
		  if popVariablesMath3.Text = "Numbers" then
		    popVariablesMath1.PopulateWithNumbers
		    popVariablesMath2.PopulateWithNumbers
		    lbl1.Text = "If This Number:"
		    lbl2.Text = "This Number:"
		  end if
		  
		  if popVariablesMath3.Text = "Text" then
		    popVariablesMath1.PopulateWithText
		    popVariablesMath2.PopulateWithText
		    lbl1.Text = "If This Text:"
		    lbl2.Text = "This Text:"
		  end if
		  
		  popVariablesMath1.SelectVariableByName(ilObjectIfThen(frmProject.chartProject.selectedFrame.tag).MathVariable1)
		  
		  popVariablesMath2.SelectVariableByName(ilObjectIfThen(frmProject.chartProject.selectedFrame.tag).MathVariable2)
		  
		  popVariablesCustomMessage.SelectVariableByName(ilObjectIfThen(frmProject.chartProject.selectedFrame.tag).CustomMessage)
		  if popVariablesCustomMessage.ListIndex = -1 then
		    popVariablesCustomMessage.ListIndex = 0
		  end if
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub PopulateTheCustomMessage()
		  isPopulating = true
		  
		  popVariablesCustomMessage.DeleteAllRows
		  
		  if popVariablesMath3.Text = "Numbers" then
		    popVariablesCustomMessage.AddRow "="
		    popVariablesCustomMessage.AddRow ">"
		    popVariablesCustomMessage.AddRow "<"
		    popVariablesCustomMessage.AddRow "<>"
		  end if
		  
		  if popVariablesMath3.Text = "Text" then
		    popVariablesCustomMessage.AddRow "="
		    popVariablesCustomMessage.AddRow "<>"
		  end if
		  
		  isPopulating = false
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub VarTypeChanged()
		  if popVariablesMath3.Text = "Numbers" then
		    popVariablesMath1.PopulateWithNumbers
		    popVariablesMath2.PopulateWithNumbers
		    lbl1.Text = "If This Number:"
		    lbl2.Text = "This Number:"
		  end if
		  
		  if popVariablesMath3.Text = "Text" then
		    popVariablesMath1.PopulateWithText
		    popVariablesMath2.PopulateWithText
		    lbl1.Text = "If This Text:"
		    lbl2.Text = "This Text:"
		  end if
		  
		  popVariablesMath1.SelectVariableByName(ilObjectIfThen(frmProject.chartProject.selectedFrame.tag).MathVariable1)
		  
		  popVariablesMath2.SelectVariableByName(ilObjectIfThen(frmProject.chartProject.selectedFrame.tag).MathVariable2)
		  
		  popVariablesCustomMessage.SelectVariableByName(ilObjectIfThen(frmProject.chartProject.selectedFrame.tag).CustomMessage)
		  if popVariablesCustomMessage.ListIndex = -1 then
		    popVariablesCustomMessage.ListIndex = 0
		  end if
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		isPopulating As Boolean
	#tag EndProperty


#tag EndWindowCode

#tag Events popVariablesMath1
	#tag Event
		Sub Change()
		  ilObjectIfThen(frmProject.chartProject.selectedFrame.tag).MathVariable1 = me.Text
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events popVariablesCustomMessage
	#tag Event
		Sub Change()
