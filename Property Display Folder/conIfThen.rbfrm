		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events popVariablesMath3
	#tag Event
		Sub Change()
		  ilObjectIfThen(frmProject.chartProject.selectedFrame.tag).MathVariable3 = me.Text
		  PopulateTheCustomMessage
		  VarTypeChanged
		End Sub
	#tag EndEvent
#tag EndEvents
