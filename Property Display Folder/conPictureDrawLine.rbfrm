#tag WindowCode
	#tag Event
		Sub Open()
		  popPicture.PopulateWithPictures
		  popPicture.SelectVariableByName(ilObjectPictureDrawLine(frmProject.chartProject.selectedFrame.tag).OriginVariable)
		  
		  popColor.PopulateWithColors
		  popColor.SelectVariableByName(ilObjectPictureDrawLine(frmProject.chartProject.selectedFrame.tag).MessageFromVariable)
		  
		  popX1.PopulateWithNumbers
		  popX1.SelectVariableByName(ilObjectPictureDrawLine(frmProject.chartProject.selectedFrame.tag).MathVariable1)
		  
		  popY1.PopulateWithNumbers
		  popY1.SelectVariableByName(ilObjectPictureDrawLine(frmProject.chartProject.selectedFrame.tag).MathVariable2)
		  
		  popX2.PopulateWithNumbers
		  popX2.SelectVariableByName(ilObjectPictureDrawLine(frmProject.chartProject.selectedFrame.tag).MathVariable3)
		  
		  popY2.PopulateWithNumbers
		  popY2.SelectVariableByName(ilObjectPictureDrawLine(frmProject.chartProject.selectedFrame.tag).MathVariable4)
		  
		  
		End Sub
	#tag EndEvent


#tag EndWindowCode

