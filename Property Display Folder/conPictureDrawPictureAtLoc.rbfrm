#tag WindowCode
	#tag Event
		Sub Open()
		  popOnWindow.PopulateWithWindows
		  popOnWindow.SelectVariableByName(ilObjectPictureDrawPicture(frmProject.chartProject.selectedFrame.tag).varWindowName)
		  
		  popLabelControlName.PopulateWithControlsOfTypeFromWindow(ilObjectPictureDrawPicture(frmProject.chartProject.selectedFrame.tag).varWindowName, 6)
		  popLabelControlName.SelectVariableByName(ilObjectPictureDrawPicture(frmProject.chartProject.selectedFrame.tag).varControlName)
		  
		  popPicture.PopulateWithPictures
		  popPicture.SelectVariableByName(ilObjectPictureDrawPicture(frmProject.chartProject.selectedFrame.tag).MessageFromVariable)
		  
		  
		  popX.PopulateWithNumbers
		  popX.SelectVariableByName(ilObjectPictureDrawPicture(frmProject.chartProject.selectedFrame.tag).MathVariable1)
		  
		  popY.PopulateWithNumbers
		  popY.SelectVariableByName(ilObjectPictureDrawPicture(frmProject.chartProject.selectedFrame.tag).MathVariable2)
		End Sub
	#tag EndEvent


#tag EndWindowCode

#tag Events popPicture
	#tag Event
		Sub Change()
		  ilObjectPictureDrawPicture(frmProject.chartProject.selectedFrame.tag).MessageFromVariable = me.Text
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events popOnWindow
	#tag Event
		Sub Change()
		  ilObjectPictureDrawPicture(frmProject.chartProject.selectedFrame.tag).varWindowName = me.Text
		  if me.Text <> "" then
		    popLabelControlName.PopulateWithControlsOfTypeFromWindow(ilObjectPictureDrawPicture(frmProject.chartProject.selectedFrame.tag).varWindowName, 6)
		    popLabelControlName.SelectVariableByName(ilObjectPictureDrawPicture(frmProject.chartProject.selectedFrame.tag).varControlName)
		    
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events popLabelControlName
	#tag Event
		Sub Change()
		  ilObjectPictureDrawPicture(frmProject.chartProject.selectedFrame.tag).varControlName = me.Text
		  
		End Sub
	#tag EndEvent
#tag EndEvents
